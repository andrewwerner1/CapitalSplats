<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CapitalSplatsRacquetball.Web</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap-theme.css" />
    <link rel="stylesheet" href="~/lib/jquery-bootgrid/dist/css/jquery.bootgrid.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Capital Splats Racquetball</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="About">About</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Contact">Contact</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Score" asp-action="Index">Scores</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - CapitalSplatsRacquetball.Web - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <script src="http://malsup.github.com/jquery.form.js"></script>    @*used for form ajaxsubmit*@

    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="https://use.fontawesome.com/9cbcc7e7e1.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @*jmk ajax stuff*@
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.min.js"
            integrity="sha256-VazP97ZCwtekAsvgPBSUwPFKdrwD3unUfSGVYrahUqU="
            crossorigin="anonymous"></script>
    <script src="~/lib/jmkAjaxStuff/jquery.ba-outside-events.min.js"></script>
    <script src="~/lib/jmkAjaxStuff/jquery.ba-resize.js"></script>
    <script src="~/lib/jmkAjaxStuff/jQuery.jmkTabs-2.0.0.js"></script>
    <script src="~/lib/jmkAjaxStuff/jquery.validate.js"></script>
    <script src="~/lib/jmkAjaxStuff//json2.js"></script>
    <script src="~/lib/jmkAjaxStuff/jquery.jmkAjaxDialog-1.0.0.js"></script>
    <script src="~/lib/jmkAjaxStuff/jquery.jmkAjaxGridView-1.0.0.js"></script>
    <script src="~/lib/jmkAjaxStuff/jQuery.jmkMainMenu-1.1.2.min.js"></script>
    <script src="~/lib/jmkAjaxStuff/jQuery.jmkSelectableTable-1.0.1.js"></script>
    <script src="~/lib/jmkAjaxStuff/jQuery.jmkSortableTable-1.0.0.js"></script>


    @RenderSection("Scripts", required: false)
</body>
</html>
